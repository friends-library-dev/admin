{
  "name": "@friends-library/orders",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "main": "index.js",
  "scripts": {
    "graphql:codegen": "npm run graphql:codegen:schema && npm run graphql:codegen:types && npm run graphql:codegen:cleanup",
    "start": "snowpack dev",
    "build": "snowpack build",
    "compile": "fldev ts:compile",
    "lint": "fldev lint",
    "lint:fix": "fldev lint:fix",
    "ts:check": "fldev ts:check",
    "format": "fldev format",
    "test": "true",
    "graphql:codegen:schema": "apollo client:download-schema --endpoint=http://127.0.0.1:8080/graphql ./schema.graphql",
    "graphql:codegen:types": "rm -rf src/graphql && npx apollo client:codegen --outputFlat --passthroughCustomScalars --localSchemaFile=schema.graphql --target=typescript --tagName=gql src/graphql",
    "graphql:codegen:cleanup": "rm -f schema.graphql && $(npm run format; npm run lint:fix) &> /dev/null; true"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/client": "^3.5.8",
    "@headlessui/react": "^1.4.3",
    "@heroicons/react": "^1.0.4",
    "classnames": "^2.3.1",
    "immer": "^9.0.12",
    "lodash.get": "^4.4.2",
    "lodash.isequal": "^4.5.0",
    "lodash.omit": "^4.5.0",
    "lodash.set": "^4.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@friends-library/dev": "^4.5.0",
    "@friends-library/theme": "^1.2.0",
    "@friends-library/types": "^6.0.0",
    "@htc-class/storylite": "^1.0.1",
    "@snowpack/plugin-dotenv": "^2.2.0",
    "@snowpack/plugin-postcss": "^1.4.3",
    "@tailwindcss/forms": "^0.4.0",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.isequal": "^4.5.5",
    "@types/lodash.omit": "^4.5.6",
    "@types/lodash.set": "^4.3.6",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^8.3.1",
    "postcss": "^8.3.6",
    "snowpack": "^3.8.8",
    "tailwindcss": "^3.0.18"
  }
}
